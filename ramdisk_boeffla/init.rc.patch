From: ZaneZam <cyxman@yahoo.com>
Date: Sat, 17 May 2014 19:44:02 +0200
Subject: [PATCH] init.rc patch

---
 init.rc | 14 +++++++-------
 1 file changed, 7 insertions(+), 7 deletions(-)

diff --git a/init.rc b/init.rc
index 83a554c..ed99354 100755
--- a/init.rc
+++ b/init.rc
@@ -784,13 +784,6 @@ service powersnd /system/bin/samsungpowersoundplay
     disabled
     oneshot
 
-service bootanim /system/bin/bootanimation
-    class main
-    user graphics
-    group graphics system
-    disabled
-    oneshot
-
 service installd /system/bin/installd
     class main
     socket installd stream 600 system system
@@ -875,3 +868,10 @@ service icd /system/bin/icd
 on property:init.svc.media=restarting
     exec icd_check
     start icd
+
+# Call Boeffla-Kernel init script
+service boeffla-init /sbin/boeffla-init.sh
+    class main
+    user root
+    group root
+    oneshot
